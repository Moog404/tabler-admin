<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}!!!TEST TABLER!!!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        <link rel="stylesheet" href="https://unpkg.com/@tabler/core@latest/dist/css/tabler.min.css">
        <link rel="stylesheet" href="https://unpkg.com/@tabler/core@1.0.0-beta3/dist/css/tabler-flags.min.css">
        <script src="https://unpkg.com/@tabler/core@latest/dist/js/tabler.min.js" defer></script>
        {#        <script src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js" defer></script>#}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="antialiased" {{ stimulus_controller('symfony/ux-swup/swup') }}
            data-theme="slide"
    >
        {% if block('login_body') is defined %}
            {{ block('login_body') }}
        {% else %}
            <div class="wrapper">
                {% include '/backend/_sidebar.html.twig' %}
                {% include '/backend/_navbar.html.twig' %}
                <div id="swup" class="page-wrapper">
                <div class="page-wrapper">
                    {% block body %}{% endblock %}
                </div>
            </div>
        {% endif %}

    </body>
</html>
